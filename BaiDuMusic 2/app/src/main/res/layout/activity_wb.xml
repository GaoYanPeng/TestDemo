<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="@dimen/all_six_five"
        android:background="@color/coloBGNormal"
        >

        <TextView
            android:layout_toRightOf="@+id/we_close_view"
            android:id="@+id/we_title_view"
            android:layout_centerVertical="true"
            android:textSize="@dimen/sp_16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="adfafdsfs"
            android:textColor="@android:color/white"/>
        <ImageView
            android:layout_marginLeft="@dimen/dp_10"
            android:id="@+id/we_close_view"
            android:padding="@dimen/dp_6"
            android:layout_centerVertical="true"
            android:src="@mipmap/bt_return_nor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </RelativeLayout>

    <WebView
        android:id="@+id/aty_wb"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>